<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseGuideView">
        <attr name="numberOfChannelsToDisplay" format="integer"/>
        <attr name="verticalDividerHeight" format="dimension"/>
        <attr name="horizontalDividerHeight" format="dimension"/>
        <attr name="divider" format="reference|color"/>
        <attr name="selector" format="reference|color"/>
        <attr name="timeLineProgressIndicator" format="reference|color"/>
        <attr name="timeLineTextFormat" format="string"/>
        <attr name="timeLineTextSize" format="dimension"/>
        <attr name="oneMinuteWidth" format="dimension"/>
        <attr name="selectionType">
            <enum name="fixedOnScreen" value="0"/>
            <enum name="notFixed" value="1"/>
        </attr>
        <attr name="selectionFixedValue" format="float"/>
    </declare-styleable>
</resources>